<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="momentBoundingBox" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <GridPane fx:id="grid">
              <columnConstraints>
                  <ColumnConstraints hgrow="NEVER" />
                  <ColumnConstraints hgrow="ALWAYS" />
                <ColumnConstraints hgrow="SOMETIMES" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints vgrow="NEVER" />
              </rowConstraints>
               <children>
                  <AnchorPane fx:id="categoryDropper" GridPane.columnIndex="1">
                     <children>
                        <BorderPane fx:id="momentBody" prefWidth="120.0" prefHeight="50" styleClass="modelMomentCard" stylesheets="@../../../css/application.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <center>
                              <BorderPane BorderPane.alignment="CENTER">
                                 <BorderPane.margin>
                                    <Insets />
                                 </BorderPane.margin>
                                 <padding>
                                    <Insets bottom="10.0" />
                                 </padding>
                              </BorderPane>
                           </center>
                           <top>
                              <HBox alignment="CENTER_RIGHT" prefWidth="120.0" styleClass="moment-header" BorderPane.alignment="CENTER">
                                 <children>
                                    <HBox fx:id="nameBox" alignment="CENTER" HBox.hgrow="ALWAYS">
                                       <children>
                                          <Label fx:id="momentName" text="New Moment" HBox.hgrow="ALWAYS">

                                          </Label>
                                       </children>
                                    </HBox>
                                 </children>
                                 <padding>
                                    <Insets bottom="3.0" top="3.0" />
                                 </padding>
                              </HBox>
                           </top>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" />
                           </padding>
                           <bottom>
                              <VBox fx:id="categoryContainer" BorderPane.alignment="CENTER" />
                           </bottom>
                        </BorderPane>
                     </children>
                  </AnchorPane>
               </children>
            </GridPane>
            <BorderPane fx:id="momentContainer">
               <padding>
                  <Insets right="15.0" />
               </padding>
               <center>
                  <HBox fx:id="childrenBox" alignment="CENTER" BorderPane.alignment="CENTER" />
               </center>
            </BorderPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
