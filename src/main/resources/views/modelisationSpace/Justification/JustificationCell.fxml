    </center>
   <left>
        <HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
         <MenuButton fx:id="menuButton" mnemonicParsing="false" styleClass="threedotsTreeview" stylesheets="@../../../css/application.css" />
         <ImageView fx:id="descriptemeDndLogo" fitHeight="150.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../../../images/hasExtractIconDisabled.png" />
            </image>
         </ImageView>
         <BorderPane.margin>
            <Insets left="5.0" right="10.0" />
         </BorderPane.margin>
        </HBox>
