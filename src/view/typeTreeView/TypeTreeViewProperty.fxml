<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="typePane" maxHeight="30.0" prefHeight="30.0" styleClass="tree-view-right" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <Label fx:id="nomType" text="Label" BorderPane.alignment="CENTER">
         <graphic>
            <ImageView fx:id="propertyIcon" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
         </graphic></Label>
   </left>
   <padding>
      <Insets bottom="3.0" right="8.0" top="3.0" />
   </padding>
   <center>
      <HBox styleClass="tree-view-right" alignment="CENTER_LEFT">
         <children>
            <Button fx:id="rename" mnemonicParsing="false" onAction="#rename" />
            <Button fx:id="deleteProperty" mnemonicParsing="false" onAction="#deleteProperty" />
         </children>
         <BorderPane.margin>
            <Insets left="15.0" />
         </BorderPane.margin>
      </HBox>
   </center>
</BorderPane>
