<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="typePane" maxHeight="30.0" minWidth="300.0" prefHeight="30.0" prefWidth="300.0" styleClass="tree-view-right" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <Label fx:id="nomType" text="Label" BorderPane.alignment="CENTER">
         <graphic>
            <ImageView fx:id="folderIcon" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
         </graphic></Label>
   </left>
   <right>
      <HBox alignment="CENTER_LEFT" styleClass="tree-view-right" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="rename" mnemonicParsing="false" onAction="#rename" />
            <Button fx:id="addFolder" mnemonicParsing="false" onAction="#addFolder" />
            <Button fx:id="addClass" mnemonicParsing="false" onAction="#addClass" />
            <Button fx:id="deleteFolder" mnemonicParsing="false" onAction="#deleteFolder" />
         </children>
         <BorderPane.margin>
            <Insets left="15.0" />
         </BorderPane.margin>
      </HBox>
   </right>
</BorderPane>
