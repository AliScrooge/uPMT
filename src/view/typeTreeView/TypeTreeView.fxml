<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="typePane" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <Label fx:id="nomType" text="Label" BorderPane.alignment="CENTER">
         <graphic>
            <ImageView fx:id="rootIcon" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
         </graphic></Label>
   </left>
   <padding>
      <Insets bottom="3.0" right="8.0" top="3.0" />
   </padding>
   <center>
      <HBox alignment="CENTER_LEFT">
         <BorderPane.margin>
            <Insets left="15.0" />
         </BorderPane.margin>
         <children>
            <Button fx:id="rename" mnemonicParsing="false" onAction="#rename" />
            <Button fx:id="addFolder" mnemonicParsing="false" onAction="#addFolder" />
            <Button fx:id="addClass" mnemonicParsing="false" onAction="#addClass" />
         </children>
      </HBox>
   </center>
</BorderPane>
