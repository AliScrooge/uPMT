<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="interviewPane" styleClass="tree-view-right" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <Label fx:id="nomEntretien" maxHeight="20.0" minHeight="20.0" prefHeight="20.0" text="Label" BorderPane.alignment="CENTER" />
   </left>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="3.0" right="8.0" top="3.0" />
   </padding>
   <right>
      <HBox styleClass="tree-view-right" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="renameInterview" mnemonicParsing="false" onAction="#renameInter" />
            <Button fx:id="afficherDescritpeme" mnemonicParsing="false" onAction="#showInterview" />
            <Button fx:id="deleteInterview" mnemonicParsing="false" onAction="#deleteInter" />
         </children>
         <BorderPane.margin>
            <Insets left="15.0" />
         </BorderPane.margin>
      </HBox>
   </right>
</BorderPane>
