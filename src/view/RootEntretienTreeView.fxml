<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane styleClass="tree-view-right" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <Label fx:id="nomRoot" maxHeight="20.0" minHeight="20.0" prefHeight="20.0" text="Label" BorderPane.alignment="CENTER">
         <graphic>
            <ImageView fx:id="rootIcon" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
         </graphic>
         <BorderPane.margin>
            <Insets left="5.0" right="5.0" />
         </BorderPane.margin>
         <padding>
            <Insets left="5.0" />
         </padding></Label>
   </left>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="3.0" right="8.0" top="3.0" />
   </padding>
   <center>
      <HBox BorderPane.alignment="CENTER" styleClass="tree-view-right">
         <children>
            <Button fx:id="addInterview" mnemonicParsing="false" onAction="#newInterview">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </center>
</BorderPane>
